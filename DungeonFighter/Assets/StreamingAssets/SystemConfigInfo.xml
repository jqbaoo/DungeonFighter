<!-- 游戏项目整体配置核心参数 -->
<SystemConfigInfo>
  <!-- Log日志核心参数 -->
  <!-- 日志路径 -->
  <LogPath>H:\DungeonFighterLog.txt</LogPath>
  <!-- 日志的状态：开发模式 Develop/指定代码输出模式  Special/部署模式 Deploy/停止模式 Stop -->
  <LogState>Stop</LogState>
  <!-- 日志最大容量 -->
  <LogMaxCapacity>3000</LogMaxCapacity>
  <!-- 日志的缓存处理 -->
  <LogBufferNumber>1</LogBufferNumber>
  
  <!-- 其他系统核心参数 -->
  
</SystemConfigInfo>